[titles]
label= 
#command=~/.config/i3/scripts/titles.sh
command=echo $(cat /proc/$(xdotool getwindowpid $(xdotool getwindowfocus))/comm)
interval=1
color=#9FF076

[mediaplayer]
label= 
#label=   ♪
command=/usr/share/i3blocks/mediaplayer
interval=7
instance=pragha
color=#35BCE6

[battery]
label=  Bat:
#label=   
command=~/.config/sway/scripts/battery.sh     
interval=23
color=#3CE697


[temperature]
label=  Temp:
#label=   
command=~/.config/sway/scripts/temperature
interval=7
#T_WARN=70
#T_CRIT=90
#SENSOR_CHIP="nvme-pci-0300"
color=#41E8CC

[memory]
label=  Mem:
#label=   
command=~/.config/sway/scripts/memory
interval=5
color=#38D2E0

[cpu_usage]
label=  CPU:
#label=   
command=/usr/share/i3blocks/cpu_usage
interval=3
color=#3EB5F0

[disk]
label=  Disk:
#label=   
command=~/.config/sway/scripts/disk
interval=31
color=#7FA6F4

[weather]
label= 
#label=   
command=curl -Ss 'https://wttr.in?0&T&Q' | cut -c 16- | head -2 | xargs echo
interval=360
color=#B3A0FA

[volume]
label=  Vol:
#label=   
command=/usr/share/i3blocks/volume
interval=3
signal=10
color=#E28DF7

[time]
label= 
#label=   
#command=date '+%a %b %d, %H:%M'
command=~/.config/sway/scripts/clock
interval=13
color=#FF9CD5

